{
  "content_scripts": [
    {
      "js": [
        "autopagerize.user.js"
      ], 
      "matches": [
        "http://*/*", 
        "https://*/*"
      ],
      "run_at": "document_end"
    }
  ], 
  "description": "A browser Extension for auto loading paginated web pages. AutoPagerize use in many web site, and provide efficiently web browsing.", 
  "id": "38C37D51986565D5334E62EB0D056C10A68D69C7", 
  "name": "AutoPagerize for Chrome", 
  "permissions": [
    "http://wedata.net/*"
  ], 
  "background_page": "background.html",
  "options_page": "options.html",
  "update_url": "http://autopagerize.net/files/updates.xml",
  "version": "0.0.5",
  "icons": {
     "16":  "icons/icon16.png",
     "32":  "icons/icon32.png",
     "48":  "icons/icon48.png",
     "128": "icons/icon128.png"
  }
}
